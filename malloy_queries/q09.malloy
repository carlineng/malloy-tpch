import "tpch.malloy"

query: lineitem -> {
  group_by:
    nation is supplier.nation.n_name
    o_year is orders.o_orderdate.year

  aggregate:
    amount is sum(l_extendedprice * (1 - l_discount) - (partsupp.ps_supplycost * l_quantity))

  where:
    part.p_name ~ '%green%'

  order_by:
    nation
    o_year desc
  limit: 100

} 
