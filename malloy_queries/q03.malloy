import "tpch.malloy"

query: lineitem -> {
  group_by:
    l_orderkey
    orders.o_orderdate
    orders.o_shippriority

  aggregate:
    revenue is sum(l_extendedprice * (1 - l_discount))

  where:
    orders.customer.c_mktsegment = 'BUILDING'
    and orders.o_orderdate < @1995-03-15
    and l_shipdate > @1995-03-15

  order_by:
    revenue desc
    o_orderdate

  limit: 10
}